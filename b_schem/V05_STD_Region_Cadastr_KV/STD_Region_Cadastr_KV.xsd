<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!--Справочник "Права"-->
	<xs:include schemaLocation="dRights.xsd"/>
	<!--Справочник "Ограничения (обременения) прав"-->
	<xs:include schemaLocation="dEncumbrances.xsd"/>
	<!--Справочник "Разрешенное использование"-->
	<xs:include schemaLocation="dUtilizations.xsd"/>
	<!--Справочник "Единицы измерений по ОКЕИ"-->
	<xs:include schemaLocation="dUnit.xsd"/>
	<!--Справочник "Статус объекта недвижимости"-->
	<xs:include schemaLocation="dStates.xsd"/>
	<!--Справочник "Вид земельного участка"-->
	<xs:include schemaLocation="dParcels.xsd"/>
	<!--Справочник "Категории земель"-->
	<xs:include schemaLocation="dCategories.xsd"/>
	<!--Справочник способов образования-->
	<xs:include schemaLocation="dMethod.xsd"/>
	<!--Виды объектов ГКН и ЕГРП-->
	<xs:include schemaLocation="dRealty.xsd"/>
	<!--Классификатор "Целевое назначение лесов"-->
	<xs:include schemaLocation="dForest_Use.xsd"/>
	<!--Классификатор "Природные объекты"-->
	<xs:include schemaLocation="dNatural_Objects.xsd"/>
	<!--Подсхема Документ -->
	<xs:include schemaLocation="R04_Document.xsd"/>
	<!--Подсхема АДРЕС-->
	<xs:include schemaLocation="V037_STD_Address.xsd"/>
	<!--5 Версия схемы (Приказ Минэкономразвития №831) ПЕРЕЧЕНЬ ИЗМЕНЕНИЙ-->
	<!--Подключена 4-я версия подсхемы "Документ" и новый справочник dAppliedFiles-->
	<!--Удалено AdditionalName (дополнительное наименование)-->
	<!--tArea: удален AreaCode и справочник dArea, площадь сделана не множественной-->
	<!--Utilization сделан не множественным. В типе tUtilization удален атрибут Fakt-->
	<!--Раздел Ground_Payments заменен разделом CadastralCost, отключен справочник dGroundPayments-->
	<!--Добавлен новый раздел tNatural_Object и новые классификаторы dNatural_Objects, dForest_Use-->
	<!--"Элементарный" тип для части элемента - точка (фиксированное значение), удалена координата Z-->
	<!--Отключен справочник dGeopoint_Zacrep, поле текстовое-->
	<!--Доработан реестр исполнителей работ (Reestr_Contractor)-->
	<!--Удалены Sender и Recipient-->
	<!--CadastralNumber земельного участка сделан обязательным-->
	<!--Для правообладателя ЗУ добавлен блок Contact (связь с правообладателем)-->
	<!--11 февраля: В разделе Права поле Type (код права) и раздел Contact (связь с правообладателем) сделаны необязательными-->
	<!--Изменена длина поля значения кадастровой стоимости (2 знака после запятой)-->
	<!--Удален элемент eDocument, а атрибуты CodeType и Version перенесены в корневой-->
	<!--19 февраля: Ред. аннотации к Contactor - Кадастровый инженер-->
	<!--20 февраля: Элемент кадастровый номер квартала (CadastralNumber_Parent) переименован в CadastralBlock-->
	<!--25 февраля: добален раздел OffspringParcel-->
	<!--25 февраля Доработан tOrdinate и Su_Nmb-->
	<!--В разделе Кадастровый инженер удалено поле Organization-->
	<!--Редакция аннотаций-->
	<!--19.04.2013: для SpecZone удалено ограничение на длину поля-->
	<!--РАЗДЕЛЫ СХЕМЫ:-->
	<xs:element name="Region_Cadastr_Vidimus_KV">
		<xs:annotation>
			<xs:documentation>ОСНОВНОЙ КОРНЕВОЙ ЭЛЕМЕНТ</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Package">
					<xs:annotation>
						<xs:documentation>Пакет ДОКУМЕНТОВ</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="Parcels" minOccurs="0"/>
							<xs:element name="Certification_Doc" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Сведения об удостоверении документа</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Organization">
											<xs:annotation>
												<xs:documentation>Наименование органа кадастрового учета</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="500"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="Date" type="xs:date">
											<xs:annotation>
												<xs:documentation>Дата регистрации кадастровой выписки</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="Number">
											<xs:annotation>
												<xs:documentation>Номер документа</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="100"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="Appointment">
											<xs:annotation>
												<xs:documentation>Должность удостоверяющего</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="255"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="FIO">
											<xs:annotation>
												<xs:documentation>Инициалы, фамилия удостоверяющего</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="100"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Reestr_Contractors" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Реестр исполнителей  работ</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Reestr_Contractor" type="tReestr_Contractor" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Реестр исполнителей работ</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="Coord_Systems" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="CodeType" type="xs:string" use="required" fixed="114">
				<xs:annotation>
					<xs:documentation>Тип программы</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Version" type="xs:string" use="required" fixed="05">
				<xs:annotation>
					<xs:documentation>Версия схемы</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
		<xs:key name="GUID_FL">
			<xs:selector xpath="./Reestr_Contractors/Reestr_Contractor/Cadastral_Engineer"/>
			<xs:field xpath="GUID_FL"/>
		</xs:key>
		<xs:key name="GUID_UL">
			<xs:selector xpath="./Reestr_Contractors/Reestr_Contractor/Cadastral_Organization"/>
			<xs:field xpath="GUID_UL"/>
		</xs:key>
		<xs:keyref name="GUID_FL_Cadastral_Engineer" refer="GUID_UL">
			<xs:selector xpath="./Reestr_Contractors/Reestr_Contractor/Cadastral_Engineer"/>
			<xs:field xpath="GUID_UL"/>
		</xs:keyref>
		<xs:keyref name="GUID_FL_Contactor" refer="GUID_FL">
			<xs:selector xpath="./Package/Parcels/Parcel/Contactor"/>
			<xs:field xpath="GUID_FL"/>
		</xs:keyref>
		<xs:keyref name="GUID_UL_Contactor" refer="GUID_UL">
			<xs:selector xpath="./Package/Parcels/Parcel/Contactor"/>
			<xs:field xpath="GUID_UL"/>
		</xs:keyref>
	</xs:element>
	<xs:element name="Parcels">
		<xs:annotation>
			<xs:documentation>Участки</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Parcel" type="tParcel" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Участок</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="OffspringParcel" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Участки-потомки ЗУ</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="Entity_Spatial"/>
						</xs:sequence>
						<xs:attribute name="CadastralNumber" use="optional">
							<xs:annotation>
								<xs:documentation>Кадастровый номер</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="40"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="tParcel">
		<xs:annotation>
			<xs:documentation>Описание земельного участка</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CadastralBlock">
				<xs:annotation>
					<xs:documentation>Кадастровый номер квартала</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="40"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Contactor" type="tContactor" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Кадастровый инженер</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Old_CadastralNumbers" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Ранее присвоенные кадастровые или условные номера</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Prev_CadastralNumbers" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Номера земельных участков, из которых образован данный участок</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="CadastralNumber" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Кадастровый номер</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="40"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Area">
				<xs:annotation>
					<xs:documentation>Площадь земельного участка</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="tArea">
							<xs:sequence>
								<xs:element name="Innccuracy" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Погрешность вычисления</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="xs:decimal">
											<xs:totalDigits value="20"/>
											<xs:fractionDigits value="2"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Location" type="tLocation">
				<xs:annotation>
					<xs:documentation>Адрес (местоположение) участка</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Category" type="tCategory">
				<xs:annotation>
					<xs:documentation>Категория земель</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Utilization" type="tUtilization">
				<xs:annotation>
					<xs:documentation>Разрешенное использование участка</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Natural_Objects" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Природные объекты на участке</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Natural_Object" type="tNatural_Object" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Природные объекты на участке</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Rights" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Права</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Right" type="tRight" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Право</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="SubParcels" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Части участка</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="SubParcel" type="tSubParcel" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Часть участка</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Contours" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Контуры многоконтурного участка</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Contour" type="tContour" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Контур</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Encumbrances" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения об ограничениях (обременениях) прав</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Encumbrance" type="tEncumbrance" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Сведения об ограничениях (обременениях) прав</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="CadastralCost" type="tCadastralCost" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Кадастровая стоимость</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="Entity_Spatial" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Описание местоположения границ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OwnerNeighbours" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Правообладатели смежных земельных участков</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="OwnerNeighbour" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Правообладатель смежного ЗУ</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Neighbour" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>Связь с правообладателем смежного ЗУ</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="ContactAddress">
													<xs:annotation>
														<xs:documentation>Контактный адрес правообладателя</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="500"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="CadastralNumber" use="required">
									<xs:annotation>
										<xs:documentation>Кадастровый номер смежного участка</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:maxLength value="40"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="SpecZones" minOccurs="0"/>
			<xs:element name="SpecialNote" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Особые отметки (строка 15)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="CadastralNumber" use="required">
			<xs:annotation>
				<xs:documentation>Кадастровый номер</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="40"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Name" type="dParcels" use="optional">
			<xs:annotation>
				<xs:documentation>Наименование участка</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="State" type="dStates">
			<xs:annotation>
				<xs:documentation>Статус записи о ЗУ по справочнику</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DateExpiry" type="xs:date">
			<xs:annotation>
				<xs:documentation>Дата истечения временного характера сведений о ЗУ</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DateCreated" type="xs:date">
			<xs:annotation>
				<xs:documentation>Дата постановки на учет</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DateRemoved" type="xs:date">
			<xs:annotation>
				<xs:documentation>Дата снятия с учета</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DateCreatedDoc" type="xs:date">
			<xs:annotation>
				<xs:documentation>Дата постановки на учет по документу (для ранее учтенных участков)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="tSubParcel">
		<xs:annotation>
			<xs:documentation>Описание части земельного участка</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Area" type="tArea">
				<xs:annotation>
					<xs:documentation>Площадь</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice minOccurs="0">
				<xs:element name="Encumbrance" type="tEncumbrance">
					<xs:annotation>
						<xs:documentation>Сведения о части, сформированной в связи с наложением обременений</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Object_Entry">
					<xs:annotation>
						<xs:documentation>Объект, входящий в состав единого землепользования</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="CadastralNumber" use="required">
							<xs:annotation>
								<xs:documentation>Кадастровый номер</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="40"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="Type" type="dRealty" use="required">
							<xs:annotation>
								<xs:documentation>Тип по справочнику</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:element ref="Entity_Spatial" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Описание местоположения границ</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Number_Record">
			<xs:annotation>
				<xs:documentation>Учетный номер части</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="40"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Full" type="xs:boolean">
			<xs:annotation>
				<xs:documentation>Весь участок (если часть совпадает со всем участком - то 1, иначе 0)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DateCreated" type="xs:date">
			<xs:annotation>
				<xs:documentation>Дата возникновения части</xs:documentation>
			</xs:annotation>
